{
  "name": "scuttlebutt.rs",
  "version": "1.0.0",
  "description": "documentation for the Scuttlebutt protocol implemented in Rust",
  "main": "index.js",
  "scripts": {
    "install": "gitbook install",
    "start": "gitbook serve",
    "build": "gitbook build",
    "push": "gh-pages --repo git@github.com:ssbrs/ssbrs.github.io --branch master --dist _book",
    "deploy": "npm-run-all -s build push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ssbrs/scuttlebutt.rs.git"
  },
  "keywords": [
    "documentation",
    "scuttlebutt",
    "ssb",
    "rust",
    "p2p",
    "decentralized",
    "decentralised",
    "communication",
    "protocol",
    "social",
    "network"
  ],
  "author": "rusty sunrise choir",
  "license": "CC-BY-4.0",
  "bugs": {
    "url": "https://github.com/ssbrs/scuttlebutt.rs/issues"
  },
  "homepage": "https://github.com/ssbrs/scuttlebutt.rs#readme",
  "devDependencies": {
    "gh-pages": "^1.2.0",
    "gitbook-cli": "^2.3.2",
    "npm-run-all": "^4.1.3"
  }
}
